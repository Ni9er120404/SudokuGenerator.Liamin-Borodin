<Window x:Class="SudokuGenerator.Liamin_Borodin.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d"
		Title="Sudoku Generator" 
		Height="275"
		Width="275">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition/>
			<RowDefinition/>
		</Grid.RowDefinitions>

		<Button x:Name="Start" Content="Начать" Click="Start_Click" Grid.Row="0"/>
		<Button x:Name="Rules" Content="Правила" Click="Rules_Click" Grid.Row="1"/>
		<Button x:Name="Esc" Content="Выход" Click="Esc_Click" Grid.Row="2"/>
	</Grid>
</Window>